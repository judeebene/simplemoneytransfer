<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shareqube.judeben.mmt" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Navigation"
            android:label="@string/title_activity_navigation" >
        </activity>
        <activity
            android:name=".SendMoney"
            android:label="@string/title_activity_send_money" >
        </activity>
        <activity
            android:name=".Account"
            android:label="@string/title_activity_account" >
        </activity>
        <activity
            android:name=".ConfirmTransfer"
            android:label="@string/title_activity_confirm_transfer" >
        </activity>
        <activity
            android:name=".CheckBalance"
            android:label="@string/title_activity_check_balance" >
        </activity>
        <activity
            android:name=".RecieveMoney"
            android:label="@string/title_activity_recieve_money" >
        </activity>
        <activity
            android:name=".TransferCompleted"
            android:label="@string/title_activity_transfer_completed" >
        </activity>
        <activity
            android:name=".AccountContinue"
            android:label="@string/title_activity_account_continue" >
        </activity>
    </application>

</manifest>
